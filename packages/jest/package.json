{
  "name": "@firestore-emulator/jest",
  "version": "0.3.3",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YutaUra/firestore-emulator.git",
    "directory": "packages/jest"
  },
  "bugs": {
    "url": "https://github.com/YutaUra/firestore-emulator/issues/new"
  },
  "keywords": [
    "firestore",
    "firestore emulator",
    "firebase",
    "firebase admin",
    "jest",
    "testing"
  ],
  "author": {
    "name": "Yuta Ura",
    "url": "http://github.com/YutaUra"
  },
  "scripts": {
    "build": "tsup ./src/ --sourcemap --clean --format esm,cjs --no-splitting && tsc --emitDeclarationOnly",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "@firestore-emulator/server": "workspace:*",
    "find-free-ports": "3.1.1"
  },
  "devDependencies": {
    "@jest/environment": "30.0.4",
    "@quramy/jest-prisma": "1.8.1",
    "@shared/biome-config": "workspace:^",
    "@shared/typescript-config": "workspace:^",
    "@types/node": "22.16.3",
    "jest": "30.0.4",
    "jest-environment-node": "30.0.4",
    "tsup": "8.5.0",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "@jest/environment": "^29 || ^30",
    "@quramy/jest-prisma-node": "1.8.1",
    "jest": "^29 || ^30",
    "jest-environment-node": "^29 || ^30"
  },
  "peerDependenciesMeta": {
    "@quramy/jest-prisma-node": {
      "optional": true
    }
  }
}