{
  "name": "@firestore-emulator/server",
  "version": "0.3.10",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YutaUra/firestore-emulator.git",
    "directory": "packages/server"
  },
  "bugs": {
    "url": "https://github.com/YutaUra/firestore-emulator/issues/new"
  },
  "keywords": [
    "firestore",
    "firestore emulator",
    "google cloud firestore",
    "protocol buffers",
    "protobuf",
    "grpc"
  ],
  "author": {
    "name": "Yuta Ura",
    "url": "http://github.com/YutaUra"
  },
  "scripts": {
    "build": "tsup ./src/ --sourcemap --clean --format esm,cjs --no-splitting && tsc --emitDeclarationOnly",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "type:check": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@firestore-emulator/proto": "workspace:*",
    "@grpc/grpc-js": "1.14.1",
    "assert-never": "1.4.0",
    "google-protobuf": "4.0.1",
    "immer": "10.2.0",
    "typesafe-event-emitter": "1.1.0"
  },
  "devDependencies": {
    "@google-cloud/firestore": "8.0.0",
    "@shared/biome-config": "workspace:^",
    "@shared/typescript-config": "workspace:^",
    "@types/google-protobuf": "3.15.12",
    "@types/node": "24.10.1",
    "grpc-tools": "1.13.0",
    "protoc-gen-ts": "0.8.7",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.12"
  }
}